    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "antd": "^3.21.4",
    "babel-plugin-import": "^1.12.0",
    "braft-editor": "^2.3.7",
    "customize-cra": "^0.5.0",
    "emoji-mart": "^2.11.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "nprogress": "^0.2.0",
    "react": "^16.9.0",
    "react-amap": "^1.2.8",
    "react-app-rewired": "^2.1.3",
    "react-contextmenu": "^2.11.0",
    "react-dom": "^16.9.0",
